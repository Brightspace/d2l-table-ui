@@ set
	endpoint http://localhost:8080/components/d2l-table/demo/simple.html
	factory  selenium grid http://${USERNAME}:${ACCESS_KEY}@ondemand.saucelabs.com:80/wd/hub --name "d2l-table galen" --page

# See https://github.com/SeleniumHQ/selenium/blob/master/java/client/src/org/openqa/selenium/Platform.java for mapping
@@ table browsers
	| browserName     | browserFactory | browserArgs                                                     |
	| chrome-windows  | ${factory}     | --browser Chrome --platform "WIN10"                             |
	| firefox-windows | ${factory}     | --browser Firefox --platform "WIN10"                            |
	| ie11-windows    | ${factory}     | --browser "internet explorer" --version "11" --platform "WIN10" |
	| ie10-windows    | ${factory}     | --browser "internet explorer" --version "10" --platform "WIN8"  |
	| edge-windows    | ${factory}     | --browser microsoftedge --platform "WIN10"                      |
	| chrome-mac      | ${factory}     | --browser Chrome --platform "EL_CAPITAN"                        |
	| safari-mac      | ${factory}     | --browser Safari --platform "EL_CAPITAN"                        |
	| firefox-mac     | ${factory}     | --browser Firefox --platform "EL_CAPITAN"                       |
