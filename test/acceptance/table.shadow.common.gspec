@objects

	small                 .screenshot-small
		head              thead
			tr-*          tr
		body              tbody
			tr-*          tr
		foot              tfoot
			tr-*          tr

	simple                .screenshot-simple
		head              thead
			tr-*          tr
		body              tbody
			tr-*          tr
		foot              tfoot
			tr-*          tr

	notbody               .screenshot-notbody
		tr-*              tr

	rows                  .screenshot-rows
		tr-*              tr

	rows-styled           .screenshot-rows-styled
		tr-*              tr:not(:last-child)

	sort                  body /deep/ .screenshot-sort
		tr-*              tr
		sort-button-*     thead d2l-table-col-sort-button


	wide                  .screenshot-wide
		left-action       .screenshot-wide /deep/ .left > d2l-icon-button
		right-action      .screenshot-wide /deep/ .right > d2l-icon-button
		tr-*              tr

	no-column-border      .screenshot-no-column-border
		head              thead
			tr-*          tr
		body              tbody
			tr-*          tr

	no-column-border-styled .screenshot-no-column-border-styled
		head                thead
			tr-*            tr
		body                tbody
			tr-*            tr

	tr-last-of-type       .screenshot-tr-last-of-type
		head              thead
			tr-*          tr
		body              tbody
			tr-*          tr
